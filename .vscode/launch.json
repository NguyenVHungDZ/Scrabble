{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SDL App",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/SDL/bin/main.exe",
      "cwd": "${workspaceFolder}/SDL/bin",
      "stopAtEntry": false,
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/Desktop/LTNC/SDLGame/src",
      "program": "d:/Desktop/LTNC/SDLGame/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}